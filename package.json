    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cypress:run",
    "test:e2e:base": "start-server-and-test develop http://localhost:8000 cypress:base",
    "functions": "netlify-lambda serve functions",
    "clean": "rimraf public/ .cache/",
    "revert": "node ./scripts/revertConfig.js",
    "bootstrap": "netlify dev:exec node ./scripts/bootstrap-fauna-database.js",
    "build:functions": "netlify-lambda build functions -c ./webpack.config.js",
    "benchmark": "node scripts/benchmark.js https://blog.tubone-project24.xyz $(git rev-parse HEAD)",
